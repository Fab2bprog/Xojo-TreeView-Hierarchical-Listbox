#tag Events BTN_OK
	#tag Event
		Sub Pressed()
		  if Pop_Emoji.SelectedRowText.Length>0 then
		    WinInput_Emoji = Pop_Emoji.SelectedRowText
		    WinInput_IsOk=true
		  else
		    // Warning Leave the space character here or an emoji, NO empty string
		    WinInput_Emoji ="üìÅ"
		  end if
		  
		  ForceExit=true
		  self.close
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events BTN_Cancel
	#tag Event
		Sub Pressed()
		  WinInput_InputValue=""
		  WinInput_IsOk=False
		  
		  self.close
		End Sub
	#tag EndEvent
#tag EndEvents
